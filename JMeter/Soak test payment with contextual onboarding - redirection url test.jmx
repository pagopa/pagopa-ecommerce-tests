          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Search for card payment method">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

def body = new JsonSlurper().parseText(prev.getResponseDataAsString())

def cardPaymentMethod = body.paymentMethods.find{it.paymentTypeCode == &apos;CP&apos;}

if(cardPaymentMethod != null){
	vars.put(&apos;PAYMENT_METHOD_ID&apos;, cardPaymentMethod.id)
} </stringProp>

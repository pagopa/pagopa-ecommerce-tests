          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="Extract contextual onboarding session token" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper

def body = new JsonSlurper().parseText(prev.getResponseDataAsString())

def fragments = body.redirectUrl.split(&quot;#&quot;)[1].split(&quot;&amp;&quot;).collectEntries{[it.split(&quot;=&quot;)[0], it.split(&quot;=&quot;)[1]]}
log.info(&quot;fragments: &quot; + fragments)
vars.put(&quot;CONTEXTUAL_ONBOARDING_SESSION_TOKEN&quot;, fragments[&quot;sessionToken&quot;])</stringProp>

                  "  } else {",
                  "    console.warn('Could not extract sessionToken from redirectUrl, falling back to main SESSION_TOKEN');",
                  "    pm.environment.set('WALLET_JWT_TOKEN', pm.environment.get('SESSION_TOKEN'));",

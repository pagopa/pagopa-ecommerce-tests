		{
			"name": "Create wallet session",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const sdk = require('postman-collection');",
							"",
							"pm.test(\"[ecommerce for IO - Contextual Onboarding] - Create wallet session\", function () {",
							"  pm.response.to.have.status(200);",
							"  const responseJson = pm.response.json();",
							"  ",
							"  pm.expect(responseJson.orderId).to.be.a('string');",
							"  pm.expect(responseJson.sessionData).to.be.an('object');",
							"  pm.expect(responseJson.sessionData.paymentMethodType).to.equal('cards');",
							"  pm.expect(responseJson.sessionData.cardFormFields).to.be.an('array');",
							"  ",
							"  pm.environment.set('ORDER_ID', responseJson.orderId);",
							"  ",
							"  // extract npg session data from response",
							"  saveNpgData(responseJson);",
							"  ",
							"  console.log(`Wallet session created - orderId: ${responseJson.orderId}`);",
							"});",
							"",
							"function saveNpgData(response) {",
							"  console.log('extracting npg session data...');",
							"  const fieldUrl = response.sessionData.cardFormFields[0].src;",
							"  ",
							"  const parsedUrl = new sdk.Url(fieldUrl).toJSON();",
							"  const correlationId = parsedUrl.query.find((queryParam) => queryParam.key === 'correlationid').value;",
							"  const sessionId = parsedUrl.query.find((queryParam) => queryParam.key === 'sessionid').value.replaceAll('%2F', '/').replaceAll('%2B', '+').replaceAll('%3D', '=');",
							"  const fieldId = parsedUrl.query.find((queryParam) => queryParam.key === 'id').value;",
							"  ",
							"  console.log(`Correlation ID: [${correlationId}]`);",
							"  console.log(`Session ID: [${sessionId}]`);",
							"  console.log(`Field ID: [${fieldId}]`);",
							"  ",
							"  pm.environment.set('NPG_CORRELATION_ID', correlationId);",
							"  pm.environment.set('NPG_SESSION_ID', sessionId);",
							"  pm.environment.set('NPG_IFRAME_FIELD_URL', fieldUrl);",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
							"value": "{{WALLET_JWT_TOKEN}}",
							"type": "string"
						}
					]
				},
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"paymentMethodType\": \"cards\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{HOSTNAME}}/webview-payment-wallet/v1/wallets/{{WALLET_ID}}/sessions",
					"host": [
						"{{HOSTNAME}}"
					],
					"path": [
